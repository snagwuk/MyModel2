<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>MyModel2</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <jsp-config>
	  <jsp-property-group>
	  	<url-pattern>/view/*</url-pattern>
	  		<include-prelude>/common/head.jsp</include-prelude>
	  </jsp-property-group>
  </jsp-config>
  
   <servlet>
    <servlet-name>boardController</servlet-name>
    <servlet-class>controller.BoardController</servlet-class>
     <!-- <init-param>
        <param-name>configFile</param-name>
      <param-value>/WEB-INF/commandBoard.properties</param-value>
    </init-param> -->
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>boardController</servlet-name>
    <url-pattern>/board/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>memberController</servlet-name>
    <servlet-class>controller.MemberController</servlet-class>
     <init-param>
        <param-name>configFile</param-name>
      <param-value>/WEB-INF/commandMember.properties</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>memberController</servlet-name>
    <url-pattern>/member/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>chartController</servlet-name>
    <servlet-class>controller.ChartController</servlet-class>
     <init-param>
        <param-name>configFile</param-name>
      <param-value>/WEB-INF/commandChart.properties</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>chartController</servlet-name>
    <url-pattern>/chart/*</url-pattern>
  </servlet-mapping>
  
  
  
  
</web-app>